{
  "name": "Notion plus",
  "description": "Desktop notifications for notion reminders",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "notifications", "identity"],
  "host_permissions": ["https://api.notion.com/"],
  "action": {
    "default_popup": "popup-sign-in.html"
  },
  "background": {
    "service_worker": "bg-worker.js"
  }
}